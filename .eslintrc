{
  "plugins": ["react", "prettier"],
  "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "react/jsx-filename-extension": [
      "warn",
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
